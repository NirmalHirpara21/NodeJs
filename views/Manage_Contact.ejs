<table class="table">
  <tr>
    <td><h1>Manage Contact</h1></td>
    <td><a href="/" class="btn btn-primary">Dashboard</a></td>
    <td><a href="/Add_Contact" class="btn btn-primary">Add Contact</a></td>
    <td><a href="/View_Contact" class="btn btn-primary">View Contact</a></td>
    <td><a href="/Manage_Account" class="btn btn-primary">Manage Account</a></td>
    <td><a href="/logout" class="btn btn-primary">Logout</a></td>
  </tr>
</table>


<form method="post">
  <table align="center" border="4" >
    <tr>
      <td><span class="input-group-addon">Name:</span></td>
      <td><input type="text" name="name" placeholder="Enter Name" class="form-control" value="<%= data.name %>" required>
    </tr>
    <tr>
      <td><span class="input-group-addon">Contact:</span></td>
      <td><input type="text" name="contact" placeholder="Enter Contact" class="form-control" value="<%= data.contact %>" required>
    </tr>
    <tr>
      <td><span class="input-group-addon">Gender:</span></td>
      <td>        
        <input type="radio" id="male" name="gender" value="male">Male
        <input type="radio" id="female" name="gender" value="female">Female
      </td>
    </tr>
    <tr>
      <td colspan="2" align="center">
        <input type="submit" name="save" value="Update" class="btn btn-primary" align="'center">
      </td>
    </tr>
  </table>
</form>




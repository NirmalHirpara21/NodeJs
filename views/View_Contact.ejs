<table class="table">
  <tr>
    <td><h1>View Contact</h1></td>
    <td><a href="/" class="btn btn-primary">Dashboard</a></td>
    <td><a href="/Add_Contact" class="btn btn-primary">Add Contact</a></td>
    <td><a href="/View_Contact" class="btn btn-primary">View Contact</a></td>
    <td><a href="/Manage_Account" class="btn btn-primary">Manage Account</a></td>
    <td><a href="/logout" class="btn btn-primary">Logout</a></td>
  </tr>
</table>

<table class="table">
  <th>Id</th>
  <th>Name</th>
  <th>Contact</th>
  <th>Gender</th>
  <th>Delete</th>
  <th>Update</th>
    <% data.forEach(function(res){ %> 
      <tr>
        <td><%= res.id %></td>
        <td><%= res.name %></td>
        <td><%= res.contact %></td>
        <td><%= res.gender %></td>
        <td><a href="Delete_Contact/<%= res.id%>" class="btn btn-danger">Delete</a></td>
        <td><a href="Manage_Contact/<%= res.id%>" class="btn btn-primary">Update</a></td>
        
        
      </tr>
  <%  }); %>
</table>


